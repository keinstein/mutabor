mutabor.app/Contents/PkgInfo: mutabor$(EXEEXT) $(top_srcdir)/src/mac/carbon/Info.plist.in $(top_srcdir)/src/mac/carbon/wxmac.icns
	mkdir -p mutabor.app/Contents
	mkdir -p mutabor.app/Contents/MacOS
	mkdir -p mutabor.app/Contents/Resources


	sed -e "s/IDENTIFIER/`echo $(srcdir) | sed -e 's,\.\./,,g' | sed -e 's,/,.,g'`/" \
	-e "s/EXECUTABLE/mutabor/" \
	-e "s/VERSION/$(WX_VERSION)/" \
	$(top_srcdir)/src/mac/carbon/Info.plist.in >mutabor.app/Contents/Info.plist


	echo -n "APPL????" >mutabor.app/Contents/PkgInfo


	ln -f mutabor$(EXEEXT) mutabor.app/Contents/MacOS/mutabor


	cp -f $(top_srcdir)/src/mac/carbon/wxmac.icns mutabor.app/Contents/Resources/wxmac.icns

